<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RealEstate.Pages.CustomTabbedPage"
            xmlns:local="clr-namespace:RealEstate.Pages"
            xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.ToolbarPlacement="Bottom"
            SelectedTabColor="#1d97ab"
            UnselectedTabColor="#78909c">

    <NavigationPage Title="Home" IconImageSource="home_icon" BarBackgroundColor="#1d97ab">
        <x:Arguments>
            <local:HomePage />

        </x:Arguments>
    </NavigationPage>
    <local:BookmarkPage Title="Saved" IconImageSource="bookmark_icon"/>
    <local:SettingsPage Title="Settings" IconImageSource="settings_icon"/>
</TabbedPage>